<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>
<!-- Admin -->
<% if admin_signed_in? %>
<%= link_to 'Admin', admins_w_welcome_index_path, class: "btn btn-success" %>
<% else %>
<%= link_to 'Admin Sign-in', admins_w_welcome_index_path, class: "btn btn-dark" %>
